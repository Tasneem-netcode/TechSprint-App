# EcoSphere AI Environment Configuration
# Copy this file to .env and fill in your API keys

# OpenAQ API (optional - free tier available at https://openaq.org)
OPENAQ_API_KEY=your_openaq_api_key_here

# OpenWeatherMap API (free tier at https://openweathermap.org/api)
OPENWEATHERMAP_API_KEY=your_openweathermap_api_key_here

# Google Gemini API (from https://aistudio.google.com)
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development
